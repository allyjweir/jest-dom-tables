{
  "name": "jest-dom-tables",
  "version": "0.0.0-semantically-released",
  "description": "Additional jest matchers to test HTML tables",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "format": "kcd-scripts format",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "files": ["dist", "extend-expect.js"],
  "keywords": ["testing", "dom", "jest", "jsdom"],
  "author": "Joe Wright <joe@joejag.com> (https://code.joejag.com/)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.1",
    "chalk": "^2.4.1",
    "css": "^2.2.3",
    "css.escape": "^1.5.1",
    "jest-diff": "^24.0.0",
    "jest-matcher-utils": "^24.0.0",
    "lodash": "^4.17.11",
    "pretty-format": "^24.0.0",
    "redent": "^3.0.0"
  },
  "devDependencies": {
    "jest-watch-select-projects": "^0.1.2",
    "jsdom": "^15.1.0",
    "kcd-scripts": "^1.4.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "babel/no-invalid-this": "off",
      "import/prefer-default-export": "off",
      "import/no-unassigned-import": "off"
    }
  },
  "eslintIgnore": ["node_modules", "coverage", "dist"]
}
